{
  "name": "Xboard-React-Vite",
  "description": "A modern React + Ant Design 5 + Vite theme for Xboard",
  "version": "1.0.0",
  "images": "",
  "configs": [
    {
      "label": "Theme Color",
      "placeholder": "Select theme color",
      "field_name": "theme_color",
      "field_type": "select",
      "select_options": {
        "default": "Green (Default)",
        "blue": "Blue",
        "black": "Black"
      },
      "default_value": "default"
    },
    {
      "label": "Background Image",
      "placeholder": "Enter background image URL",
      "field_name": "background_url",
      "field_type": "input"
    },
    {
      "label": "Custom Footer HTML",
      "placeholder": "Add custom JS or HTML",
      "field_name": "custom_html",
      "field_type": "textarea"
    }
  ]
}
